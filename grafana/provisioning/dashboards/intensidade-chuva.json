      "gridPos": {"h": 10, "w": 12, "x": 12, "y": 14},
      "targets": [
        {
          "datasource": {"type": "postgres", "uid": "postgresql-inmet"},
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT intensidade_chuva, COALESCE(ROUND(AVG(temperatura_ar_c), 2), 0) as temp_media, COALESCE(ROUND(AVG(umidade_rel_horaria_pct), 2), 0) as umidade_media, COALESCE(ROUND(AVG(pressao_estacao_mb), 2), 0) as pressao_media, COALESCE(ROUND(AVG(vento_velocidade_ms), 2), 0) as vento_medio, COALESCE(ROUND(AVG(precipitacao_mm), 2), 0) as precip_media FROM dados_meteorologicos WHERE intensidade_chuva IS NOT NULL GROUP BY intensidade_chuva UNION ALL SELECT 'sem_chuva' as intensidade_chuva, COALESCE(ROUND(AVG(temperatura_ar_c), 2), 0) as temp_media, COALESCE(ROUND(AVG(umidade_rel_horaria_pct), 2), 0) as umidade_media, COALESCE(ROUND(AVG(pressao_estacao_mb), 2), 0) as pressao_media, COALESCE(ROUND(AVG(vento_velocidade_ms), 2), 0) as vento_medio, COALESCE(ROUND(AVG(precipitacao_mm), 2), 0) as precip_media FROM dados_meteorologicos WHERE intensidade_chuva IS NULL OR intensidade_chuva = 'sem_chuva' GROUP BY 'sem_chuva' UNION ALL SELECT 'leve' as intensidade_chuva, 0, 0, 0, 0, 0 WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'leve') UNION ALL SELECT 'moderada' as intensidade_chuva, 0, 0, 0, 0, 0 WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'moderada') UNION ALL SELECT 'forte' as intensidade_chuva, 0, 0, 0, 0, 0 WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'forte') ORDER BY CASE intensidade_chuva WHEN 'sem_chuva' THEN 1 WHEN 'leve' THEN 2 WHEN 'moderada' THEN 3 WHEN 'forte' THEN 4 END;",
          "refId": "A"
        }
      ],
