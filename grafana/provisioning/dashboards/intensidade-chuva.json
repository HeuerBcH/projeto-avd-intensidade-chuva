      {
        "id": 5,
        "title": "Distribuição de Intensidade de Chuva",
        "type": "barchart",
      "gridPos": {"h": 9, "w": 24, "x": 0, "y": 5},
        "targets": [
          {
            "datasource": {"type": "postgres", "uid": "postgresql-inmet"},
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
          "rawSql": "SELECT intensidade_chuva, COALESCE(COUNT(*), 0) as total_registros FROM dados_meteorologicos WHERE intensidade_chuva IS NOT NULL AND intensidade_chuva != 'sem_chuva' GROUP BY intensidade_chuva UNION ALL SELECT 'leve' as intensidade_chuva, 0 as total_registros WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'leve') UNION ALL SELECT 'moderada' as intensidade_chuva, 0 as total_registros WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'moderada') UNION ALL SELECT 'forte' as intensidade_chuva, 0 as total_registros WHERE NOT EXISTS (SELECT 1 FROM dados_meteorologicos WHERE intensidade_chuva = 'forte') ORDER BY CASE intensidade_chuva WHEN 'leve' THEN 1 WHEN 'moderada' THEN 2 WHEN 'forte' THEN 3 END;",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
          "color": {"mode": "thresholds"},
            "custom": {
              "hideFrom": {"tooltip": false, "viz": false, "legend": false}
            },
            "mappings": [
              {
                "options": {
                "leve": {"text": "Leve", "color": "#56A64B"},
                "moderada": {"text": "Moderada", "color": "#FADE2A"},
                "forte": {"text": "Forte", "color": "#E24D42"}
